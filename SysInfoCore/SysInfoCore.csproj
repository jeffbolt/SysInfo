<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
		<TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
		<!-- Top-level statements are implicitly in the global namespace. The Assembly name can be renamed with this expression. -->
    <AssemblyName>$(MSBuildProjectName.Replace("Core",""))</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BetterConsoleTables" Version="1.1.3" />
  </ItemGroup>

</Project>
